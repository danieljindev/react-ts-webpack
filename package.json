{
  "name": "react-ts-webpack",
  "version": "1.0.0",
  "description": "Setup a React app with Webpack, Typescript, Eslint and Prettier",
  "main": "index.js",
  "author": "Daniel Jin",
  "license": "MIT",
  "scripts": {
    "build": "webpack --env production --config config/webpack.prod.ts --progress",
    "start": "webpack serve --env development --config config/webpack.dev.ts --open --hot",
    "eslint": "eslint src/**/*.{ts,tsx}",
    "eslint:fix": "eslint --fix src/**/*.{ts,tsx}",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,scss,md}\"",
    "format:check": "prettier --check \"./**/*.{js,jsx,ts,tsx,css,scss,md}\"",
    "lint": "eslint src/**/*.{ts,tsx} --quiet --fix"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/css-minimizer-webpack-plugin": "^3.0.1",
    "@types/mini-css-extract-plugin": "^1.4.3",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/webpack-dev-server": "^3.11.4",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.10",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "node-sass": "^6.0.0",
    "prettier": "^2.3.1",
    "process": "^0.11.10",
    "sass-loader": "^12.1.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "eslint",
    "prettier"
  ]
}
